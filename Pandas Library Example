import pandas as pd

# Create a DataFrame from a dictionary
data = {'Name': ['Alice', 'Bob', 'Charlie'], 'Age': [25, 30, 35]}
df = pd.DataFrame(data)

# Print the DataFrame
print(df.tail)

# Access a column
print(df['Name'])

# Add a new column
df['City'] = ['New York', 'London', 'Tokyo']

# Print the updated DataFrame
print(df.head)

# Get summary statistics
print(df.describe())
